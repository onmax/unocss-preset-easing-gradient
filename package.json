{
  "name": "unocss-preset-easing-gradient",
  "version": "1.0.8",
  "packageManager": "pnpm@9.12.3",
  "description": "UnoCSS preset to create gradients with easing functions.",
  "author": "onmax",
  "license": "MIT",
  "homepage": "https://github.com/onmax/unocss-preset-easing-gradient",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onmax/unocss-preset-easing-gradient.git"
  },
  "bugs": {
    "url": "https://github.com/onmax/unocss-preset-easing-gradient/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "easing gradients"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "dev:playground": "pnpm -C ./playground dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "node --loader=ts-node/esm src/index.ts",
    "test": "node --loader=ts-node/esm bin/test.ts",
    "typecheck": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "peerDependencies": {
    "unocss": ">= 0.31.13 < 1"
  },
  "dependencies": {
    "@unocss/preset-mini": "^0.64.0",
    "@unocss/rule-utils": "^0.64.0",
    "culori": "^4.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@types/culori": "^2.1.1",
    "@unocss/core": "^0.64.0",
    "@unocss/eslint-plugin": "^0.65.3",
    "@unocss/preset-rem-to-px": "^0.65.3",
    "@unocss/reset": "^0.64.0",
    "bumpp": "^9.8.1",
    "colorjs.io": "^0.6.0-alpha.1",
    "eslint": "^9.14.0",
    "eslint-plugin-format": "^0.1.2",
    "lint-staged": "^15.2.10",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.64.0",
    "vitepress": "^1.5.0",
    "vue": "^3.5.13"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
